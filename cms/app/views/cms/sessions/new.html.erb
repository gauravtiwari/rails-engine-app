<h1> Login</h1>
<%= form_for(Cms::Administrator.new, url: sessions_path) do |f| %>
  <%= f.email_field :email, autofocus: true %>
  <%= f.password_field :password, autocomplete: "off", class: "forgot-password" %>
  <%= f.submit 'Login', class: "login-button" %>
<% end %>

<% if flash[:notice] %>
  <div class="notice">
    <%= flash[:notice] %>
  </div>
<% end %>
